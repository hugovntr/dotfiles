{
  "title": "Home Row Mods",
  "rules": [
    {
      "description": "ASD -> Shift, Alt, Command",
      "manipulators": [
        {
          "type": "basic",
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 150
          },
          "from": {
            "key_code": "a",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to_delayed_action": {
            "to_if_canceled": {
              "key_code": "a"
            },
            "to_if_invoked": {
              "key_code": "vk_none"
            }
          },
          "to_if_held_down": {
            "key_code": "left_shift",
            "halt": true
          },
          "to_if_alone": {
            "halt": true,
            "key_code": "a"
          }
        },
        {
          "type": "basic",
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 150
          },
          "from": {
            "key_code": "s",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to_delayed_action": {
            "to_if_canceled": {
              "key_code": "s"
            },
            "to_if_invoked": {
              "key_code": "vk_none"
            }
          },
          "to_if_held_down": {
            "key_code": "left_alt",
            "halt": true
          },
          "to_if_alone": {
            "halt": true,
            "key_code": "s"
          }
        },
        {
          "type": "basic",
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 150
          },
          "from": {
            "key_code": "d",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to_delayed_action": {
            "to_if_canceled": {
              "key_code": "d"
            },
            "to_if_invoked": {
              "key_code": "vk_none"
            }
          },
          "to_if_held_down": {
            "key_code": "left_command",
            "halt": true
          },
          "to_if_alone": {
            "halt": true,
            "key_code": "d"
          }
        }
      ]
    }
  ]
}
